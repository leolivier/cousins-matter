	"version": "0.2.0",
	"configurations": [

		{
			"name": "Python Debugger: Django",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
				"runserver"
			],
			"django": true,
			"autoStartBrowser": false,
			"justMyCode": false,
		},
		{
			"name": "Debug Django Test",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
					"test",
					"members.tests.tests_member"
//					"accounts"
			],
			"django": true,
			"autoStartBrowser": false,
			"justMyCode": false,
	},
	]
}